<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item>
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/_3sdp" />
            <size
                android:width="@dimen/_14sdp"
                android:height="@dimen/_14sdp"/>
            <stroke
                android:width="1dp"
                android:color="@color/check_icon_color" />
        </shape>
    </item>

    <item
        android:bottom="@dimen/_3sdp"
        android:left="@dimen/_3sdp"
        android:right="@dimen/_3sdp"
        android:top="@dimen/_3sdp">

        <shape android:shape="rectangle">
            <size
                android:width="@dimen/_8sdp"
                android:height="@dimen/_8sdp" />
            <corners android:radius="@dimen/_2sdp" />
            <solid android:color="@color/check_icon_color" />
        </shape>
    </item>
</layer-list>
